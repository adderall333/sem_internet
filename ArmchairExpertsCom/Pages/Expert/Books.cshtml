@page
@using ArmchairExpertsCom.Models
@model ArmchairExpertsCom.Pages.Expert.Books

@{
    Layout = "Shared/_ProfileLayout";
    ViewData["Title"] = "Прочитанные книги";
}

@foreach (var evaluation in Model.Evaluations)
{
